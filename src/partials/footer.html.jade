ul
  if social.facebook
    li
      a(href='https://www.facebook.com/#{social.facebook}')
        i.fa.fa-facebook
        | Facebook
  if social.twitter
    li
      a(href="https://www.twitter.com/#{social.twitter}")
        i.fa.fa-twitter
        | Twitter
  if social.googleplus
    li
      a(href="https://plus.google.com/+#{social.googleplus}")
        i.fa.fa-googleplus
        | Google+
  if social.youtube
    li
      a(href="https://www.youtube.com/c/#{social.youtube}")
        i.icon-youtube
        | YouTube
  if social.github
    li
      a(href="https://github.com/#{social.github}")
        i.icon-igithub
        | GitHub
  if social.vine
    li
      a(href="https://vine.co/tags/#{social.vine}")
        i.icon-vine
        | Vine
